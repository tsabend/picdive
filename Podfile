# Uncomment this line to define a global platform for your project
platform :ios, '8.0'

use_frameworks!
xcodeproj 'PictoGif.xcodeproj'
target 'PictoGif' do
 pod "CGRectExtensions"
 pod "SwiftGifOrigin"
 pod "Reveal-iOS-SDK", :configurations => ['Debug']
 pod "pop", "~> 1.0"
end


target 'PictoGifTests' do
  pod "Quick"
  pod "Nimble"
end

